<h1 style="text-align: center;">All Users</h1>
<div class="listcont">

    <table class="table table-dark">
        <thead>
          <tr>
            <th scope="col">S.No</th>
            <th scope="col">Name</th>
            <th scope="col">Email</th>
            <th scope="col">Password</th>
            <th scope="col">Role</th>
            <th scope="col">Remove</th>
          </tr>
        </thead>
        <tbody>
            <%var i=0; for(user of allusers){%>
                <tr>
                    <th scope="row"><%= ++i%></th>
                    <td><%= user.name %></td>
                    <td><%= user.email %></td>
                    <td><%= user.password %></td>
    <td><a style="text-decoration: none; color:white;" href="/admin/change/<%= user.id %>"><%if(user.isadmin){%>Admin<%}else{%>Employee<%}%></a></td>
    <td><a style="text-decoration: none; color:red;" href="/admin/delete/<%= user.id %>">X</a></td>
                </tr>
            <%}%>
        </tbody>
      </table>
</div>